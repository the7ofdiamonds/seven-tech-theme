{
  "name": "seven-tech-theme",
  "version": "1.0.0",
  "description": "Wordpress Theme",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel sync watch:sass",
    "sync": "browser-sync start --proxy 'http://192.168.1.170:8000' --files '**/*.html, **/*.css, **/*.js'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "update-dev": "git push seven-tech-theme development",
    "update-prod": "git push seven-tech-theme production",
    "watch:sass": "sass --watch ./CSS/scss/main.scss ./CSS/main.css"
  },
  "author": "Jamel C. Lyons <jclyonsenterprises@gmail.com>, THE7OFDIAMONDS.TECH",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^2.29.3",
    "npm-run-all": "^4.1.5"
  }
}
